        "hasbin": "^1.2.3",
        "highlight.js": "^10.4.1",
        "laravel-mix": "^6.0.12",
        "laravel-mix-purgecss": "^6.0.0",
        "node-cmd": "^3.0.0",
        "postcss": "^8.4.4",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.81.0",
        "sass-loader": "^12.6.0",
